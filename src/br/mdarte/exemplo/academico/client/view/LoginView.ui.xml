<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		
		<table cellpadding="0" cellspacing="0">
			<tr>
				<td><g:Label ui:field="labelLogin">Login</g:Label></td>
				<td><g:TextBox ui:field="login" /></td>
			</tr>
			<tr>
				<td><g:Label ui:field="labelSenha">Senha</g:Label></td>
				<td><g:PasswordTextBox ui:field="senha" /></td>
			</tr>
		</table>
	
		<g:Button ui:field="loginButton" > Login </g:Button>

	</g:HTMLPanel>
</ui:UiBinder>