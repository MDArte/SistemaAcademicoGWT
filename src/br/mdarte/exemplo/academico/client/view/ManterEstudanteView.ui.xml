<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<g:HTMLPanel>
		<g:Hidden ui:field="idEstudante" />
		
		<table cellpadding="0" cellspacing="0">
			<tr>
				<td><g:Label ui:field="labelMatricula">Matricula</g:Label></td>
				<td><g:TextBox ui:field="matricula" /></td>
			</tr>
			<tr>
				<td><g:Label ui:field="labelNome">Nome</g:Label></td>
				<td><g:TextBox ui:field="nome" /></td>
			</tr>
		</table>
	
		<g:Button ui:field="manterButton" />

	</g:HTMLPanel>

</ui:UiBinder> 